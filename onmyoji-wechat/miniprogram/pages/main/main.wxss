#choukabg{
  background-image: url(http://ousc.me/wp-content/uploads/2019/07/chouka_bg.png);
  background-color: rgba(0,0,0,0.7);
  background-size: auto 70%;
  z-index:995;
  width:100%;
  height:100%;
  position: absolute;
  background-repeat: no-repeat;
  background-position: center bottom;
}
#choukabg.show{
  animation:show-right .5s forwards;
}
#choukabg.hide{
  visibility:visible;
  opacity: 1;
  transform: translateX(calc(50% - 190px));
  animation:hide-right .5s forwards;
}

#lihui.show{
  animation:show-left .5s forwards;
}
#lihui.hide{
  visibility:visible;
  opacity: 1;
  transform: translateX(0);
  animation:hide-left .5s forwards;
}
@keyframes show-left{
  to {
    visibility:visible;
    opacity: 1;
    transform: translateX(calc(50% - 190px))
  }
}
@keyframes hide-left{
  to {
    visibility:hidden;
    opacity: 0;
    transform: translateX(-100%);
  }
}

@keyframes show-right{
  from{
    transform: translateX(100%);
  }
  to {
    visibility:visible;
    opacity: 1;
    transform: translateX(0)
  }
}
@keyframes hide-right{
  to {
    visibility:hidden;
    opacity: 0;
    transform: translateX(100%);
  }
}
@keyframes rotate{from{transform: rotate(0deg)}
    to{transform: rotate(360deg)}
}
#loading_bg{
  background: url(http://ousc.me/wp-content/uploads/2019/07/loading.png) no-repeat;
  background-position: center center;
  background-size: 50% auto;
  animation: rotate 3s linear infinite;
  width: 100%;
  height:100%;
  position: absolute;
  z-index: 90;
}
#loading{
  background-color: #FFF;
  font-size: 12px;
  padding-top: 20px;
  color: #7e5f27;
  background-image: none;
  text-shadow: 6px 12px 6px #AAA;
}


#door_left.show{
  animation:door-left-in 2s forwards;
}
#door_left.hide{
  visibility:visible;
  opacity: 1;
  transform: translateX(0);
  animation:door-left-out 2s forwards;
}
@keyframes door-left-in{
  from{
    transform: translateX(-50%)
  }
  to {
    visibility:visible;
    opacity: 1;
    transform: translateX(0)
  }
}

@keyframes door-left-out{
  to {
    visibility:hidden;
    opacity: 0;
    transform: translateX(-50%);
  }
}

#door_right.show{
  animation:door-right-in 2s forwards;
}
#door_right.hide{
  visibility:visible;
  opacity: 1;
  transform: translateX(50%);
  animation:door-right-out 2s forwards;
}
@keyframes door-right-in{
  from{
    transform: translateX(100%)
  }
  to {
    visibility:visible;
    opacity: 1;
    transform: translateX(50%)
  }
}

@keyframes door-right-out{
  to {
    visibility:hidden;
    opacity: 0;
    transform: translateX(150%);
  }
}
#settingtitle{
  position: absolute;
  left: 50px;
  top: 40px;
}
#rolepopcard{
  position: absolute;
  top: calc(40% + 117px);
  left: calc(10% - 25px);
}
.popcard {
  z-index: 100000000;
  /* display: none; */
  width: 100rpx;
  font-size: 15rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding-left: 20rpx;
  position: relative;
  color:#a9834a;
  background-color: #321309;
  border: 1px solid #734316;
  border-radius: 5px;
  padding-top: 8px;
}
.popcard:after {
  content: '';
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  top: -11rpx;
  left: 20rpx;
  transform: rotate(45deg);
  background-color: #321309;
  border: 1px solid #734316;
  border-style: solid none none solid ;
}
.section__title{
  display: inline-block;
  width: 20%;
}
.body-view{
  display: inline-block;
  width: 80%;
  border:none;
}


/* modal */
.mask{
 position: absolute;
 left: 0;
 right: 0;
 top: 0;
 bottom: 0;
 display: flex;
 justify-content: center;
 align-items: center;
 background-color: rgba(0,0,0,0.4);
 z-index: 9999;
 font-size: 14px;
}

.modal-content{
  padding:40px;
 display: flex;
 flex-direction: column;
  width: calc(90% - 80px);
  height: calc(80% - 80px);
 font-size: 14px;
 border-radius: 10rpx;
  color:#a9834a;background-color: #321309;border: 1px solid #734316;
}

.modal-btn-wrapper{
 display: flex;
 flex-direction: row;
 height: 50rpx;
 line-height: 50rpx;
 border-top: 2rpx solid rgba(7,17,27,0.1);
}

.cancel-btn, .confirm-btn{
 flex: 1;
 height: 100rpx;
 font-size: 15px;
 line-height: 100rpx;
 text-align: center;
}

.cancel-btn{
 border-right: 2rpx solid rgba(7,17,27,0.1);
}

.main-content{
 flex: 1;
 height: 100%;
 overflow-y: hidden; 
}