}

@keyframes wave {
    0% {
        height: 100%;
        transform: rotateZ(0deg) rotateX(0deg) scale(0.2);
    }
    50% {
        height: 98%;
        transform: rotateZ(10deg) rotateX(20deg) scale(0.2);
    }
    100% {
        height: 100%;
        transform: rotateZ(0deg) rotateX(0deg) scale(0.2);
    }
}
